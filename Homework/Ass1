import java.util.Random;

public class RandomDecimalNumber {
    public static void main(String[] args) {
        Random random = new Random();

        double randomNumber = random.nextDouble() * 100;

        int integerPart = (int) randomNumber;
        double decimalPart = randomNumber - integerPart;


        char characterFromCode = (char) integerPart;

        System.out.printf("From 0 to 100 the computer picked: %.2f%n", randomNumber);
        System.out.printf("Integer Part: %d%n", integerPart);
        System.out.printf("Decimal Part: %.2f%n", decimalPart);
        System.out.printf("Character from ASCII Code: %c%n", characterFromCode);
    }
}
